    "    elif choice.lower() == 'r':\n",
    "        new_food_item = input(\"Enter the new food item name: \")\n",
    "        return get_nutrition_info(new_food_item, APP_ID, APP_KEY)\n",
    "    else:\n",
    "        print(\"Invalid choice. Skipping this food item.\")\n",
    "        return None\n",
    "\n",
    "# Your Edamam API credentials\n",
